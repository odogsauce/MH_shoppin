<h1>ALL PRODUCTS</h1>

<div class="container">
  <% @products.each do |product| %>
    <div class="card-trip" link_to product_path>
        <%= link_to "show", @products_path %>
        <%= image_tag product.photo %>
        <div class="card-trip-infos">
          <div>
           <h2><%= product.title %></h2>
          </div>
          <h2 class="card-trip-pricing"> </h2>
        </div>
    </div>
  <% end %>
</div>
